<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.native_programmer.myfavouritsongs">

    <application
        android:allowBackup="true"
        android:icon="@drawable/playbuttonfour"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ArabicSongs"
                  android:label="@string/arabicSongs"/>
        <activity android:name=".EnglishSongs"
                  android:label="@string/englishSongs"/>
        <activity android:name=".playingNowActivity"
                  android:label="@string/playingNowSongs"></activity>
    </application>

</manifest>